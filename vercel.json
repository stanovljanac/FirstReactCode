{
  "version": 2,
  "builds": [{ "src": "json-server/server.js", "use": "@vercel/node" }],
  "routes": [{ "src": "/api/(.*)", "dest": "/json-server/server.js" }],
  "functions": {
    "server.js": {
      "memory": 1024,
      "includeFiles": "db.json"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "server.js"
    }
  ]
}
